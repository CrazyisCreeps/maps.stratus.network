<falling-blocks>
    <rule>
        <filter>
            <any>
                <material>smooth brick</material>
                <material>step</material>
                <material>smooth stairs</material>
                <material>cobblestone stairs</material>
                <material>dark oak stairs</material>
                <material>wood step</material>
                <material>spruce wood stairs</material>
                <material>log</material>
                <material>bookshelf</material>
                <material>furnace</material>
                <material>burning furnace</material>
                <material>workbench</material>
                <material>web</material>
                <material>wood</material>
                <material>monster eggs</material>
                <material>fence</material>
                <material>torch</material>
            </any>
        </filter>
        <sticky>
            <any>
                <material>smooth brick</material>
                <material>step</material>
                <material>smooth stairs</material>
                <material>cobblestone stairs</material>
                <material>dark oak stairs</material>
                <material>wood step</material>
                <material>spruce wood stairs</material>
                <material>bookshelf</material>
                <material>furnace</material>
                <material>burning furnace</material>
                <material>workbench</material>
                <material>web</material>
                <material>wood</material>
                <material>monster eggs</material>
                <material>fence</material>
            </any>
        </sticky>
        <delay>2</delay>
    </rule>
</falling-blocks>

