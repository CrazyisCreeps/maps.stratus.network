<?xml version="1.0"?>
<map proto="1.3.0">    
<name>Abandoned Zoo TE</name>
<version>1.1.1</version>
<objective>Be the team with the least deaths after the time limit is reached</objective>
<authors>
	<author uuid="2fa85349-2276-4850-b7b5-d18c4f4c8376"/> <!-- Solo_Runner -->
</authors>
<contributors>
	<contributor uuid="acc5d8bf-09db-44d2-903a-4a01f8d82356" contribution="XML"/> <!-- sillybillypiggy -->
</contributors>
<teams>
	<team color="blue" max="8">Blue</team>
	<team color="dark red" max="8">Red</team>
</teams>
<kits>
	<kit name="spawn">
		<item slot="0">stone sword</item>
		<item slot="1">bow</item>
		<item slot="28" amount="10">arrow</item>
		<item slot="2" amount="64">cooked fish</item>
		<potion duration="5">heal</potion>
		<potion duration="5">damage resistance</potion>
	</kit>
	<kit name="red" parents="spawn">
		<chestplate color="cd0000" unbreakable="true">leather chestplate</chestplate>
		<helmet color="cd0000" unbreakable="true">leather helmet</helmet>
		<leggings unbreakable="true">iron leggings</leggings>
		<boots color="cd0000" unbreakable="true">leather boots</boots>
	</kit>
	<kit name="blue" parents="spawn">
		<chestplate color="0066cc" unbreakable="true">leather chestplate</chestplate>
		<helmet color="0066cc" unbreakable="true">leather helmet</helmet>
		<leggings unbreakable="true">iron leggings</leggings>
		<boots color="0066cc" unbreakable="true">leather boots</boots>
	</kit>
</kits>
<spawns>
	<spawn team="blue" kit="blue" yaw="0">
		<cuboid name="spawn-blue" min="-3,15,-76" max="4,15,-83"/>
	</spawn>
	<spawn team="red" kit="red" yaw="180">
		<cuboid name="spawn-red" min="-3,15,85" max="4,15,77"/>
	</spawn>
	<default yaw="-90">
		<point>-104,43,0</point>
	</default>
</spawns>
<regions>
	<apply block="deny-all">
		<rectangle min="-oo,-oo" max="oo,oo"/>
	</apply>
</regions>
<toolrepair>
	<tool>stone sword</tool>
	<tool>bow</tool>
</toolrepair>
<itemremove>
	<item>leather helmet</item>
	<item>leather chestplate</item>
	<item>iron leggings</item>
	<item>leather boots</item>
	<item>cooked fish</item>
	<item>glass bottle</item>
</itemremove>
<killreward>
	<item>golden apple</item>
</killreward>
<score>
	<time>12m</time>
</score>
<autorespawn/>
</map>
