    <item>leather chestplate</item>
    <item>dispenser</item>
    <item>wool</item>
</itemremove>
<timelock>on</timelock>
<maxbuildheight>25</maxbuildheight>
<hunger>
    <depletion>off</depletion>
</hunger>
<tnt>
    <dispenser-tnt-limit>16</dispenser-tnt-limit>
    <dispenser-tnt-multiplier>0.25</dispenser-tnt-multiplier>
</tnt>
<filters>
    <all id="remove-platform">
        <time>0s</time>
        <not>
            <time>20s</time>
        </not>
    </all>
    <time id="place-bridges">4m</time>
    <countdown duration="21s" message="Dropping into the map in {0}">
        <match-started/>
    </countdown>
</filters>
<structures clear="true">
    <structure id="red-platform">
        <region>
            <cuboid min="17,59,-83" size="15,2,15"/>
        </region>
    </structure>
    <structure id="blue-platform">
        <region>
            <cuboid min="17,59,-9" size="15,2,15"/>
        </region>
    </structure>
    <structure id="green-platform">
        <region>
            <cuboid min="-57,59,-83" size="15,2,15"/>
        </region>
    </structure>
    <structure id="yellow-platform">
        <region>
            <cuboid min="-57,59,-9" size="15,2,15"/>
        </region>
    </structure>
    <structure id="yellow-blue">
        <region>
            <cuboid min="-19,14,-1" size="13,1,-1"/>
        </region>
    </structure>
    <structure id="green-yellow">
        <region>
            <cuboid min="-50,14,-45" size="1,1,13"/>
        </region>
    </structure>
    <structure id="red-green">
        <region>
            <cuboid min="-6,14,-76" size="-13,1,1"/>
        </region>
    </structure>
    <structure id="blue-red">
        <region>
            <cuboid min="25,14,-32" size="-1,1,-13"/>
        </region>
    </structure>
    <dynamic structure="red-platform" offset="0,0,0" trigger="remove-platform"/>
    <dynamic structure="blue-platform" offset="0,0,0" trigger="remove-platform"/>
    <dynamic structure="green-platform" offset="0,0,0" trigger="remove-platform"/>
    <dynamic structure="yellow-platform" offset="0,0,0" trigger="remove-platform"/>
    <dynamic structure="yellow-blue" offset="0,0,0" trigger="place-bridges"/>
    <dynamic structure="green-yellow" offset="0,0,0" trigger="place-bridges"/>
    <dynamic structure="red-green" offset="0,0,0" trigger="place-bridges"/>
    <dynamic structure="blue-red" offset="0,0,0" trigger="place-bridges"/>
</structures>
</map>
