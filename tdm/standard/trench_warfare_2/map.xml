</score>
<rules>
    <rule>Switching/exchanging classses or items within a class is illegal.</rule>
</rules>
<teams>
    <team color="blue" max="40" show-name-tags="allies">Blue</team>
    <team color="dark red" max="40" show-name-tags="allies">Red</team>
</teams>
<spawns>
