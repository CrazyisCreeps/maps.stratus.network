<filters>
    <filter name="only-red">
        <team>red</team>
    </filter>
    <filter name="only-blue">
        <team>blue</team>
    </filter>
    <filter name="no-tnt">
        <deny>
            <block>tnt</block>
        </deny>
    </filter>
    <filter name="break-restrictions"> <!--Deny breaking stone brick and iron objects-->
        <deny>
            <block>iron block</block>
            <block damage="5">step</block>
            <block>redstone wire</block>
            <block>wall sign</block>
            <block>stone plate</block>
            <block>smooth brick</block>
            <block>iron fence</block>
            <block>smooth stairs</block>
            <block>wood button</block>
        </deny>
    </filter>
</filters>
<regions>
    <!--Playing Field Regions-->
    <union name="bases">
        <rectangle name="red-base" min="-143,-47" max="-73,48"/>
        <rectangle name="blue-base" min="74,-47" max="144,48"/>
    </union>
    <union name="towers">
        <circle name="red a1" center="-47,42" radius="4"/>
        <circle name="red a2" center="-47,-42" radius="4"/>
        <circle name="red b1" center="-16,42" radius="4"/>
        <circle name="red b2" center="-16,-42" radius="4"/>
        <circle name="blue a1" center="47,-42" radius="4"/>
        <circle name="blue a2" center="47,42" radius="4"/>
        <circle name="blue b1" center="16,-42" radius="4"/>
        <circle name="blue b2" center="16,42" radius="4"/>
    </union>
    <union name="playable">
        <rectangle name="main-area" min="-73,42" max="74,-41"/>
        <region name="bases"/>
        <region name="towers"/>
    </union>
    <union name="cannons">
        <cuboid name="red-cannon1" min="-115,25,26" max="-110,20,27"/>
        <cuboid name="red-cannon2" min="-115,25,14" max="-110,20,15"/>
        <cuboid name="red-cannon3" min="-115,25,-14" max="-110,20,-13"/>
        <cuboid name="red-cannon4" min="-115,25,-26" max="-110,20,-25"/>
        <cuboid name="red-cannon-outpost" min="-62,18,0" max="-57,15,1"/>
        <cuboid name="blue-cannon1" min="111,20,-26" max="116,25,-25"/>
        <cuboid name="blue-cannon2" min="111,20,-14" max="116,25,-13"/>
        <cuboid name="blue-cannon3" min="111,20,14" max="116,25,15"/>
        <cuboid name="blue-cannon4" min="111,20,26" max="116,25,27"/>
        <cuboid name="blue-cannon-outpost" min="58,15,0" max="63,18,1"/>
    </union>
    <apply block="deny-all">
        <negative name="playable-inverse">
            <region name="playable"/>
        </negative>
    </apply>
    <apply enter="only-red">
        <rectangle name="red-home" min="-143,48" max="-104,-47"/>
    </apply>
    <apply enter="only-blue">
        <rectangle name="blue-home" min="105,48" max="144,-47"/>
    </apply>
    <apply block="deny-all">
        <region name="blue-home"/>
        <region name="red-home"/>
    </apply>
    <apply block-break="break-restrictions" block="no-tnt">
        <complement>
            <region name="playable"/>
            <region name="red-home"/>
            <region name="blue-home"/>
        </complement>
    </apply>
    <apply block="allow-all">
        <region name="cannons"/>
    </apply>
    <!--Apply Kit to Regions-->
    <apply kit="Red Soldier">
        <cuboid name="red-soldier" min="-124,3,-24" max="-125,6,-19"/>
    </apply>
