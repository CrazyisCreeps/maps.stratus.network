<spawns>
    <spawns safe="true" sequential="true">
        <spawn team="blue" kit="blue" yaw="180">
            <cuboid min="-25,71,22" max="-38,126,9"/> <!-- regular tower spawn -->
            <cuboid min="-42,6,5" max="-21,6,26"/> <!-- atop the obsidian -->
            <point>1,8,0</point> <!-- last resort spawn in the center pad -->
        </spawn>
        <spawn team="red" kit="red" yaw="0">
            <cuboid min="-38,71,-10" max="-25,126,-23"/> <!-- regular tower spawn -->
            <cuboid min="-21,6,-27" max="-42,6,-6"/> <!-- atop the obsidian -->
            <point>1,8,0</point> <!-- last resort spawn in the center pad -->
        </spawn>
        <spawn team="green" kit="green" yaw="0">
            <cuboid min="7,71,9" max="-6,126,22"/> <!-- regular tower spawn -->
            <cuboid min="-10,6,26" max="11,6,5"/> <!-- atop the obsidian -->
            <point>1,8,0</point> <!-- last resort spawn in the center pad -->
        </spawn>
        <spawn team="yellow" kit="yellow" yaw="0">
            <cuboid min="-6,71,-23" max="7,126,-10"/> <!-- regular tower spawn -->
            <cuboid min="11,6,-6" max="-10,6,-27"/> <!-- atop the obsidian -->
            <point>1,8,0</point> <!-- last resort spawn in the center pad -->
        </spawn>
    </spawns>
    <default yaw="270">
        <cuboid min="45,12,-2" max="49,12,2"/>
    </default>
</spawns>
<playable>
    <rectangle min="22,-38" max="-53,37"/>
</playable>
