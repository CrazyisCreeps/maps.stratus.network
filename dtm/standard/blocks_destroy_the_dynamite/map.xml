            <block>chest</block>
            <block>furnace</block>
            <block>burning furnace</block>
            <block>trapped chest</block>
            <block>ender chest</block>
            <block>dropper</block>
            <block>dispenser</block>
            <block>hopper</block>
            <block>beacon</block>
        </not>
    </filter>
</filters>
