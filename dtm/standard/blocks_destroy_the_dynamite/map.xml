        <potion duration="1" amplifier="1" force="true">invisibility</potion>
        <potion duration="5" amplifier="3" force="true">regeneration</potion>
    </kit>
</kits>
<filters>
    <filter name="only-blue">
        <team>blue</team>
    </filter>
    <filter name="only-red">
        <team>red</team>
    </filter>
    <filter name="no-storage">
