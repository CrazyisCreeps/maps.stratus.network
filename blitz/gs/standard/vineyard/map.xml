</kits>
<regions>
    <apply kit="skull">
        <cuboid min="-oo,0,-oo" max="oo,4,oo"/>
    </apply>
    <apply kit="skull-remove">
        <cuboid min="-oo,5,-oo" max="oo,oo,oo"/>
    </apply>
</regions>
<spawns safe="true">
    <spawn angle="137,8,-79">
        <cuboid min="99,5,-53" max="157,11,-93"/>
    </spawn>
    <default yaw="179">
        <block>130.5,31,-32.5</block>
    </default>
</spawns>
</map>
