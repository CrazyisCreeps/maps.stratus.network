<include src="gs-ffa.xml"/>
<name>Vineyard</name>
<version>1.1.0</version>
<objective>FFA - Kill all the players!</objective>
<gamemode>gs</gamemode>
<authors>
    <author uuid="ba4a93aa-4935-4a71-bab2-a58f9a124e7b"/> <!-- ollyj -->
    <author uuid="dff980ed-9f69-491e-b79c-30c21350baf2"/> <!-- DoomRobotBoy -->
