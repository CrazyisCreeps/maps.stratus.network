<teams>
	<team color="blue" max="10">Blue</team>
	<team color="dark red" max="10">Red</team>
</teams>
<cores material="obsidian" leak="3" mode-changes="true">
	<core id="blue-core" team="blue">
		<sphere origin="-98.5,60,-17.5" radius="3"/>
	</core>
	<core id="red-core" team="red">
		<sphere origin="-31.5,60,-17.5" radius="3"/>
	</core>
</cores>
<modes>
    <mode after="10m" material="gold block" name="GOLD CORE MODE"/>
    <mode after="20m" material="glass" name="GLASS CORE MODE"/>
</modes>
<destroyables materials="ender stone" completion="100%" sparks="true" show-progress="true">
	<destroyables owner="red">
		<destroyable name="Left Monument">
			<cylinder base="-17.5,64,-38.5" radius="3" height="9"/>
		</destroyable>
	</destroyables>
	<destroyables owner="blue">
		<destroyable name="Left Monument">
			<cylinder base="-112.5,64,3.5" radius="3" height="9"/>
		</destroyable>
	</destroyables>
</destroyables>
<filters>
	<filter name="no-stone-brick">
		<deny>
			<all>
				<cause>explosion</cause>
				<block>smooth brick</block>
			</all>
		</deny>
	</filter>
</filters>
<regions>
	<rectangle name="all" min="-oo,-oo" max="oo,oo"/>
	<negative name="outside-map">
		<rectangle name="map" min="-4,-89" max="-126,54"/>
	</negative>
	<union name="spawns">
		<cuboid name="red-drop" min="-16,46,-14" max="-10,oo,-21"/>
		<cuboid name="blue-drop" min="-120,46,-14" max="-114,oo,-21"/>
		<cuboid name="tunnel" min="-168,147,-14" max="38,oo,-21"/>
	</union>
	<!-- Applied -->
	<apply block="no-stone-brick">
		<region name="all"/>
	</apply>
	<apply block="deny-all" message="You may not edit outside the main area!">
		<complement>
			<negative>
				<region name="map"/>
			</negative>
			<region name="spawns"/>
		</complement>
	</apply>
	<apply block="deny-all" message="You may not edit the spawn!">
		<region name="spawns"/>
	</apply>
	<apply enter="deny-all" message="You may not re-enter the spawn tunnel!">
		<region name="tunnel"/>
	</apply>
</regions>
<kits>
	<kit name="spawn">
		<item slot="0" unbreakable="true">stone sword</item>
		<item slot="1" unbreakable="true">bow</item>
		<item slot="2" unbreakable="true">diamond pickaxe</item>
		<item slot="3" unbreakable="true">stone axe</item>
		<item slot="4" amount="36">wood</item>
		<item slot="5" amount="16">glass</item>
		<item slot="7" amount="64">arrow</item>
		<item slot="8" amount="64">golden carrot</item>
		<chestplate unbreakable="true" enchantment="protection explosions">gold chestplate</chestplate>
	</kit>
	<kit name="red-kit" parents="spawn">
		<helmet color="993333" unbreakable="true">leather helmet</helmet>
		<leggings color="993333" unbreakable="true">leather leggings</leggings>
		<boots color="993333" unbreakable="true">leather boots</boots>
	</kit>
	<kit name="blue-kit" parents="spawn">
		<helmet color="334CB2" unbreakable="true">leather helmet</helmet>
		<leggings color="334CB2" unbreakable="true">leather leggings</leggings>
		<boots color="334CB2" unbreakable="true">leather boots</boots>
	</kit>
</kits>
<killreward>
	<item amount="16">arrow</item>
	<item amount="1">golden apple</item>
</killreward>
<spawns>
	<spawn team="red" yaw="90" kit="red-kit">
		<point>34.5,150,-17.5</point>
	</spawn>
	<spawn team="blue" yaw="-90" kit="blue-kit">
		<point>-164.5,150,-17.5</point>
	</spawn>
	<default yaw="0">
		<cylinder base="-64.5,89,-126.5" radius="3" height="0"/>
	</default>
</spawns>
<itemremove>
	<item>obsidian</item>
	<item>clay ball</item>
	<item>glowstone dust</item>
	<item>carpet</item>
	<item>clay ball</item>
	<item>cobble wall</item>
	<item>step</item>
	<item>iron fence</item>
	<item>wool</item>
	<item>sign</item>
	<item>redstone lamp on</item>
	<item>redstone lamp off</item>
	<item>step</item>
	<item>ender stone</item>
	<item>stone sword</item>
	<item>bow</item>
	<item>diamond pickaxe</item>
	<item>stone axe</item>
	<item>arrow</item>
	<item>golden carrot</item>
	<item>leather helmet</item>
	<item>gold chestplate</item>
	<item>leather leggings</item>
	<item>leather boots</item>
</itemremove>
<maxbuildheight>78</maxbuildheight>
</map>
