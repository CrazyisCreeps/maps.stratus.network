</itemremove>
<tool-repair>
	<tool>iron sword</tool>
	<tool>bow</tool>
	<tool>diamond pickaxe</tool>
	<tool>iron axe</tool>
	<tool>diamond spade</tool>
</tool-repair>
<maxbuildheight>26</maxbuildheight>
