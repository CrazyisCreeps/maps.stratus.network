	<item>seeds</item>
	<item>sapling</item>
	<item>book</item>
	<item>string</item>
	<item>furnace</item>
	<item>redstone block</item>
	<item>wheat</item>
	<item>water bucket</item>
	<item>arrow</item>
	<item>390</item>
	<item>380</item>
	<item>348</item>
	<item>98:2</item>
	<item>38:1</item>
